{
  "name": "repositories_manager",
  "type": "module",
  "version": "0.1.1",
  "description": "ðŸ”„ sync and manage your git repositories effortlessly.",
  "license": "MIT",
  "keywords": [
    "repositories",
    "git",
    "github",
    "gitlab",
    "automation"
  ],
  "author": {
    "name": "Lucas Vieira",
    "email": "lucasvtiradentes@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasvtiradentes/repositories_manager.git"
  },
  "bin": {
    "rp": "./dist/index.js",
    "rpu": "./dist/index.js",
    "repository_utils": "./dist/index.js"
  },
  "scripts": {
    "prepare": "husky && chmod +x ./examples/simple.sh",
    "start": "node ./dist/index.js",
    "dev": "bash ./examples/simple.sh",
    "build": "rimraf ./dist && tsc",
    "commit": "cz",
    "package_check": "npm publish --dry-run",
    "changeset": "changeset",
    "release": "pnpm run build && changeset publish"
  },
  "dependencies": {
    "commander": "12.0.0",
    "commitizen": "4.3.0",
    "fuse.js": "7.0.0",
    "inquirer": "9.2.14",
    "inquirer-autocomplete-prompt": "3.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@types/inquirer": "9.0.7",
    "@types/inquirer-autocomplete-prompt": "3.0.3",
    "@types/node": "20.11.17",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.10",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "tsx": "4.7.1",
    "typescript": "5.3.3"
  }
}
